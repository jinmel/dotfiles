{
  "permissions": {
    "allow": [
      "Bash(git:*)",
      "Bash(gh:*)",
      "Bash(cargo:*)",
      "Bash(ls:*)",
      "Bash(rustc:*)",
      "Bash(npm:*)",
      "Bash(yarn:*)",
      "Bash(npx:*)",
      "WebSearch"
    ]
  },
  "enabledPlugins": {
    "context7@claude-plugins-official": true
  },
  "attribution": {
    "pr": "",
    "commit": ""
  },
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "statusLine": {
    "type": "command",
    "command": "ccline",
    "padding": 0
  }
}
