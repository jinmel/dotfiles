[push]
	default = matching
[credential]
    helper = cache
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	required = true
	process = git-lfs filter-process
[alias]
    mr = !sh -c 'git fetch $1 merge-requests/$2/head:mr-$1-$2 && git checkout mr-$1-$2' -
[core]
    editor = nvim
	excludesfile = ~/.gitignore
[difftool]
	prompt = false
[include]
	path = ~/.gitconfig.user
[init]
	defaultBranch = main
[user]
	name = Jinsuk Park
	email = totorovirus@gmail.com
